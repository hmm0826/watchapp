<view class="container"><order-detail-shop class="vue-ref" vue-id="1" shopid="{{order.shopId}}" data-ref="shopInfo" bind:__l="__l"></order-detail-shop><view class="form-group"><custom-select class="vue-ref" vue-id="2" bottom-line title="订单号" required type="input" height="50" disabled themeColor="#666" data-ref="orderSelect" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="3" bottom-line title="姓名" required type="input" height="50" textalign="left" data-ref="customerName" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="4" bottom-line title="电话" required type="input" height="50" data-ref="customerPhone" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="5" bottom-line title="委托寄件" type="input" placeholder-text="在这输入委托寄件信息" height="50" data-ref="customerShipping" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="6" title="是否保价" height="50" list="{{select.insurance_select}}" iconflag data-ref="insurance_select" bind:__l="__l"></custom-select></view><view class="form-group"><custom-select class="vue-ref" vue-id="7" bottom-line title="品牌" required height="{{pickerHeight}}" list="{{select.brand_select}}" iconflag data-ref="brand_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="8" bottom-line title="系列" height="{{pickerHeight}}" list="{{select.series_select}}" iconflag data-ref="series_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="9" bottom-line title="型号" type="input" placeholder-text="在这输入型号" height="50" list="{{select.model_select}}" data-ref="model_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="10" bottom-line title="购买日期" required datetype="month" type="date" height="{{pickerHeight}}" iconflag defaultdate="{{defaultDate}}" data-ref="buy_date" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="11" bottom-line title="是否维修过" required height="{{pickerHeight}}" list="{{select.isrepair_select}}" iconflag data-ref="isrepair_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="12" title="购买地点" type="input" height="{{pickerHeight}}" iconflag data-ref="buyPosition" bind:__l="__l"></custom-select></view><view class="form-group"><custom-select class="vue-ref" vue-id="13" bottom-line title="手表功能" required height="{{pickerHeight}}" list="{{select.function_select}}" iconflag data-ref="function_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="14" bottom-line title="机芯类别" required height="{{pickerHeight}}" list="{{select.movement_category_select}}" iconflag data-ref="movement_category_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="15" bottom-line title="表壳材质" required height="{{pickerHeight}}" list="{{select.case_material_select}}" iconflag data-ref="case_material_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="16" bottom-line title="表带材质" required height="{{pickerHeight}}" list="{{select.strap_material_select}}" iconflag data-ref="strap_material_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="17" bottom-line title="表扣材质" required height="{{pickerHeight}}" list="{{select.buckle_material_select}}" iconflag data-ref="buckle_material_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="18" bottom-line title="外观品相" required height="{{pickerHeight}}" list="{{select.appearance_select}}" iconflag data-ref="appearance_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="19" bottom-line title="送修部件" required height="{{pickerHeight}}" list="{{select.repair_parts_select}}" iconflag data-ref="repair_parts_select" bind:__l="__l"></custom-select><custom-select class="vue-ref" vue-id="20" bottom-line title="表及按钮情况" required type="input" placeholder-text="在这输入情况描述" height="{{pickerHeight}}" data-ref="watch_button_input" bind:__l="__l"></custom-select></view><view class="form-group"